{
  "name": "vue-snap",
  "version": "0.1.0-beta.2",
  "description": "Lightweight Carousel based on CSS Scroll Snap",
  "main": "dist/vue-snap.umd.js",
  "module": "dist/vue-snap.esm.js",
  "unpkg": "dist/vue-snap.js",
  "engines": {
    "node": ">=12.13.0 <13.0.0"
  },
  "scripts": {
    "release": "yarn build:browser && yarn build:es && yarn build:umd",
    "build:browser": "rollup --config ./build/rollup.config.browser.js",
    "build:es": "rollup --config ./build/rollup.config.es.js",
    "build:umd": "rollup --config ./build/rollup.config.umd.js",
    "storybook:dev": "start-storybook -s ./static -c ./config/storybook",
    "storybook:build": "build-storybook -s ./static -c ./config/storybook",
    "lint": "eslint 'src/**/*.{js,vue}' 'config/**/*.js'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "test --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bartdominiak/vue-snap.git"
  },
  "keywords": [
    "vue",
    "vue-snap",
    "carousel",
    "slider",
    "scroll-snap"
  ],
  "author": "smugers@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bartdominiak/vue-snap/issues"
  },
  "homepage": "https://github.com/bartdominiak/vue-snap#README",
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "seamless-scroll-polyfill": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@storybook/addon-a11y": "6.0.0-beta.32",
    "@storybook/addon-actions": "6.0.0-beta.32",
    "@storybook/addon-docs": "6.0.0-beta.32",
    "@storybook/addon-knobs": "6.0.0-beta.32",
    "@storybook/addon-storysource": "6.0.0-beta.32",
    "@storybook/addon-viewport": "6.0.0-beta.32",
    "@storybook/vue": "6.0.0-beta.32",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^7.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "intersection-observer": "^0.11.0",
    "react": "*",
    "react-dom": "*",
    "rollup": "^2.18.0",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "browserslist": "> 0.25%, not dead"
}
